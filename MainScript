d:Toggle("Charge Move",function(bool)
_G.AutoChargeSpeed = bool
if bool then
_G.AutoChargeSpped = true
spawn(function()
    while _G.AutoChargeSpeed == true do
        wait()
local A_1 = "Charging"
local A_2 = true
local Event = game:GetService("ReplicatedStorage").Remotes.Control
Event:FireServer(A_1, A_2)
wait(0.2)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed= 250
game.Players.LocalPlayer.Character.Humanoid.JumpPower= 200
end
end)
end
end)


