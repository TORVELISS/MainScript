_G.AutoCharge = false;
_G.AutoMove = false;
_G.AutoChargeSpeed = false;



local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("Shinobi Final Legacy") -- Creates the window

local b = w:CreateFolder("Farming") -- Creates the folder(U will put here your buttons,etc)

local c = w:CreateFolder("Teleports")

local d = w:CreateFolder("Player")

local e = w:CreateFolder("Credits")

local f = w:CreateFolder("Settings")

b:Toggle("Auto range moves",function(bool)
    _G.AutoMove = bool
    if bool then
        loadstring(game:HttpGet(("https://raw.githubusercontent.com/TORVELISS/dsadasdasdad/main/dadssadasdsa")))()
    end
end)

b:Toggle("Auto Charge",function(bool)
    _G.AutoCharge = bool
        if bool then
            _G.AutoCharge = true;

            spawn(function()
            while _G.AutoCharge == true do
            wait(0.3)
            if game:GetService("Players").LocalPlayer.Chakra.Value < 501 then
            local A_1 = "Charging"
            local A_2 = true
            local Event = game:GetService("ReplicatedStorage").Remotes.Control
            Event:FireServer(A_1, A_2)
            -- here
            end
            if game:GetService("Players").LocalPlayer.Chakra.Value > 1299 then
            local A_1 = "Charging"
            local A_2 = false
            local Event = game:GetService("ReplicatedStorage").Remotes.Control
            Event:FireServer(A_1, A_2)
            end
            end
            end)        

    end      
end)

